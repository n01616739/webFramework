<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Airbnb Results</title>
    <link rel="stylesheet" href="/styles.css">
</head>
<body>
    <h1>Airbnb Listings</h1>
    <table>
        <thead>
            <tr>
                <th>Name</th>
                <th>Description</th>
                <th>Price</th>
                <th>Property Type</th>
            </tr>
        </thead>
        <tbody>
            <% results.forEach(result => { %>
                <tr>
                    <td><%= result.name %></td>
                    <td><%= result.description %></td>
                    <td><%= result.price %></td>
                    <td><%= result.property_type %></td>
                </tr>
            <% }) %>
        </tbody>
    </table>
</body>
</html>
